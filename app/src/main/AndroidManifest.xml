<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beautywithin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MakeupActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.beautywithin.HomeActivity">
            </meta-data> />
        </activity>
        <activity android:name=".signUpActivity" android:theme="@style/NoActionBarTheme" ></activity>
        <activity android:name=".SkincareActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.beautywithin.HomeActivity">
            </meta-data> />
        </activity>
        <activity android:name=".HomeActivity"><meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.beautywithin.signUpActivity">
        </meta-data> </activity>
        <activity android:name=".MainActivity" android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>