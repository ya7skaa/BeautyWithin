<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beautywithin">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MakeupActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.beautywithin.ui.HomeActivity">
            </meta-data>
        </activity>
        <activity android:name=".ui.signUpActivity" android:theme="@style/NoActionBarTheme" ></activity>
        <activity android:name=".ui.SkincareActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.beautywithin.ui.HomeActivity">
            </meta-data>

        </activity>
        <activity android:name=".ui.HomeActivity"><meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.beautywithin.ui.signUpActivity">
        </meta-data> </activity>
        <activity android:name=".ui.MainActivity" android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>